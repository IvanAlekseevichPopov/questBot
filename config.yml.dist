bot_token: 'bot_token_here_'
proxy: '' #leave it blank or use such format to set proxy: 'socks5://user:pass@host:port'
#cronjob for notify users
cron: '0 0 * * * *'
#delay behind bot messages to imitate real user
message_delay: 300
user_notifications:
  0: { silence_time: 1, message: 'Я ещё жив, а ты?'}
  1: { silence_time: 24, message: 'Эгей! Есть кто живой?'}
env: 'dev'
